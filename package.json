{
  "name": "playground-elm-spa",
  "version": "1.0.0",
  "description": "Playing around with Elm SPAs and structuring them for larger scale applications",
  "repository": {
    "url": "https://github.com/Tehnix/playground-elm-spa",
    "type": "git"
  },
  "main": "src/Main.elm",
  "scripts": {
    "start": "npm run dev",
    "test": "elm-test",
    "test:watch": "elm-test --watch",
    "test:verify": "elm-verify-examples --run-tests",
    "dev": "npm run elm:compile:dev && npm run elm:live",
    "build": "npm run elm:compile && npm run minify",
    "minify:legacy": "uglifyjs dist/elm.js --compress 'pure_funcs=\"F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9\",pure_getters=true,keep_fargs=false,unsafe_comps=true,unsafe=true,passes=2' --output=dist/elm.min.js --source-map url=dist/elm.min.js.map && uglifyjs dist/elm.min.js --mangle --output=dist/elm.min2.js",
    "minify": "uglifyjs dist/elm.js --compress 'pure_funcs=\"F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9\",pure_getters=true,keep_fargs=false,unsafe_comps=true,unsafe=true,passes=2' --mangle --output=dist/elm.min.js --source-map url=elm.min.js.map",
    "elm:compile": "elm make src/Main.elm --output=dist/elm.js --optimize",
    "elm:compile:dev": "elm make src/Main.elm --output=dist/elm.js",
    "elm:live": "elm-live src/Main.elm --start-page=index.dev.html --open --pushstate --port=1234 -- --output=dist/elm.js --debug"
  },
  "dependencies": {
    "elm": "0.19.1-3",
    "elm-live": "4.0.1",
    "elm-analyse": "0.16.5",
    "elm-format": "0.8.2",
    "elm-test": "0.19.1-revision2",
    "elm-verify-examples": "5.0.0",
    "uglify-js": "3.7.3",
    "source-map": "0.7.3"
  },
  "devDependencies": {},
  "keywords": []
}
